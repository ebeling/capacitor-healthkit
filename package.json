{
  "name": "@gpunkt/capacitor-healthkit",
  "version": "7.4.1",
  "type": "module",
  "description": "Capacitor plugin to read data from and write data to Apple Health",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "dist/",
    "ios/Plugin/",
    "GpunktCapacitorHealthkit.podspec"
  ],
  "author": "Perfood GmbH & Kirstin Ebeling",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@humboldt.gpunkt.magdeburg.de:/var/data/git/capacitor-healthkit"
  },
  "bugs": {
    "url": "https://github.com/perfood/capacitor-healthkit/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "apple",
    "healthkit"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --fix --format",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api CapacitorHealthkitPlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && npm run docgen && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@capacitor/core": "^7.4.1",
    "@capacitor/docgen": "^0.3.0",
    "@capacitor/ios": "^7.4.1",
    "@ionic/eslint-config": "^0.4.0",
    "@ionic/prettier-config": "^4.0.0",
    "@ionic/swiftlint-config": "^2.0.0",
    "eslint": "^9.30.1",
    "prettier": "~3.6.2",
    "prettier-plugin-java": "~2.7.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.44.2",
    "swiftlint": "^2.0.0",
    "typescript": "^5.4.3"
  },
  "peerDependencies": {
    "@capacitor/core": "^7.4.1"
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    }
  }
}
